<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Ideology</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationReplace">
          <success>Always</success>
          <xpath>/Defs/CultureDef[defName="RatkiniaTraditionCulture_Warlord"]/label</xpath>
          <value>
            <label>래키니아 문화</label>
          </value>
        </li>
        <li Class="PatchOperationReplace">
          <success>Always</success>
          <xpath>/Defs/CultureDef[defName="RatkiniaTraditionCulture_Warlord"]/description</xpath>
          <value>
            <description>래키니아의 문화입니다.</description>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>